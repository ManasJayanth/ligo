(rule
 (alias runtest)
 (deps
  (:script ../../test.sh)
  (glob_files *.mligo)
  (glob_files original/*.mligo)
   %{workspace_root}/src/passes/02-parsing/cameligo/ParserMain.exe)
 (action
  (progn
   (run bash %{script} --dialect=cameligo --test-type=fuzzing --dune-workspace=%{workspace_root})
   (diff? results.csv.expected results.csv))))
