(rule
 (alias runtest)
 (deps
  (:script ../../test.sh)
  (glob_files *.mligo)
  (glob_files original/*.mligo)
   %{workspace_root}/src/passes/02-parsing/cameligo/ParserMain.exe)
 (action
  (progn
   (run bash %{script} '*.mligo' %{workspace_root}/src/passes/02-parsing/cameligo/ParserMain.exe)
   (diff? results.csv.expected results.csv))))
