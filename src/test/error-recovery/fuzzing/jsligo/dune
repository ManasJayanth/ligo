(rule
 (alias runtest)
 (deps
  (:script ../../test.sh)
  (glob_files *.jsligo)
  (glob_files original/*.jsligo)
  (:parser ../../../../passes/02-parsing/jsligo/ParserMain.exe))
 (action
  (progn
   (run
    bash
    %{script}
    --dialect=jsligo
    --test-type=fuzzing
    --dune-workspace=%{workspace_root})
   (diff? results.csv.expected results.csv))))
