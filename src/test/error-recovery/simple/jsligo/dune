(rule
 (alias runtest)
 (deps
  (:script ../../test.sh)
  (glob_files *.jsligo)
  (glob_files original/*.jsligo)
   ../../../../passes/02-parsing/jsligo/ParserMain.exe)
 (action
  (progn
   (run bash %{script} '*.jsligo' ../../../../passes/02-parsing/jsligo/ParserMain.exe)
   (diff? results.csv.expected results.csv))))
